<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="seelaunch.seelaunch.WebBrowser"
    android:id="@+id/id_relative"
    >

    <WebView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/webview_browser"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_below="@+id/editText_search"
        android:layout_marginTop="7dp"/>




    <EditText
        android:layout_width="53dp"
        android:layout_height="48dp"
        android:id="@+id/editText_search"
        android:background="@drawable/custom_editext_search"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"

        android:textSize="20dp"
        android:inputType="textAutoComplete"

        android:layout_toLeftOf="@+id/btn_go"
        android:layout_toStartOf="@+id/btn_go"

        android:textStyle="bold"
        android:textColor="@android:color/holo_red_dark" />


    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go"
        android:id="@+id/btn_go"
        android:textAllCaps="false"
        android:background="#4CAF50"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"

        android:layout_marginRight="48dp"
        android:layout_marginEnd="48dp"
        android:layout_alignBaseline="@+id/editText_search"
        android:layout_alignBottom="@+id/editText_search"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:text=""
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btn_mainmenu"
        android:background="@drawable/main_menu_button"
        android:layout_marginLeft="67dp"
        android:layout_marginStart="67dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/editText_search"
        android:layout_toEndOf="@+id/editText_search" />


</RelativeLayout>
